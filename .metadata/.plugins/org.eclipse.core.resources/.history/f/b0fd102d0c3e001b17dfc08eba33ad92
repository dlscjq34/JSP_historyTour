package com.tour.VO;

import java.sql.Timestamp;

public class BoardVO {
	int num;
	String title;
	String writer;
	String content;
	Timestamp wrtdate;
	String hit;
	
	public BoardVO() {
		// TODO Auto-generated constructor stub
	}

	public BoardVO(int num, String title, String writer, Timestamp wrtdate, String hit) {
		this.num = num;
		this.title = title;
		this.writer = writer;
		this.wrtdate = wrtdate;
		this.hit = hit;
	}

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}

	public String getTitle() {
		return title;
	}

	public void setTitle(String title) {
		this.title = title;
	}

	public String getWriter() {
		return writer;
	}

	public void setWriter(String writer) {
		this.writer = writer;
	}

	public Timestamp getWrtdate() {
		return wrtdate;
	}

	public void setWrtdate(Timestamp wrtdate) {
		this.wrtdate = wrtdate;
	}

	public String getHit() {
		return hit;
	}

	public void setHit(String hit) {
		this.hit = hit;
	}

	@Override
	public String toString() {
		return "BoardVO [num=" + num + ", title=" + title + ", writer=" + writer + ", wrtdate=" + wrtdate + ", hit="
				+ hit + "]";
	}	
}
